    <property name="resourceConsultingAreaIdResources" type="integer" column="ResourceConsultingArea_idResources" not-null="true"/>
    <property name="resourceConsultingAreaIdConsultingArea" type="integer" column="ResourceConsultingArea_idConsultingArea" not-null="true"/>
  </class>
</hibernate-mapping>
